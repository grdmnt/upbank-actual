# Copy this file to .env and fill in values

# Up Bank webhook secret (returned when creating the webhook)
UP_WEBHOOK_SECRET=

# Up API personal access token (to fetch transaction details)
UP_API_TOKEN=

# Actual server connection
ACTUAL_SERVER_URL=
ACTUAL_PASSWORD=
# Budget Sync ID (Settings → Advanced → Sync ID)
ACTUAL_BUDGET_ID=
# Optional: end-to-end encryption password for the budget file
ACTUAL_BUDGET_ENCRYPTION_PASSWORD=
# Where to cache budget data locally
ACTUAL_DATA_DIR=.actual-data

# Map Up account IDs to Actual account IDs (JSON object)
# Example: {"1234-up-account-id":"abcd-actual-account-id"}
ACCOUNT_MAP={}

# Logging
LOG_LEVEL=info
PORT=8080
