  "private": true,
  "description": "Webhook handler to push Up Bank transactions into Actual Budget (PikaPod)",
  "main": "src/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --ext js --exec node src/index.js",
