    "list-up-accounts": "node scripts/list-up-accounts.js",
    "create-up-webhook": "node scripts/create-up-webhook.js",
    "list-up-webhooks": "node scripts/list-up-webhooks.js",
    "delete-up-webhook": "node scripts/delete-up-webhook.js",
    "ping-up-webhook": "node scripts/ping-up-webhook.js"
  },
  "dependencies": {
    "@actual-app/api": "^25.7.1",
    "axios": "^1.7.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
